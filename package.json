{
  "name": "gale.js",
  "version": "1.0.0",
  "description": "Basic static site generator, noting more, nothing less. It literally just spits out html and other files for a static site. What did you expect?",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "dev": "nodemon --inspect server.js",
    "watch": "webpack --config ./client/webpack.config.js --watch",
    "build-assets": "webpack --config ./client/webpack.config.js --mode=\"production\"",
    "build": "node ./commands/build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/M-erb/gale.js"
  },
  "keywords": [
    "Static",
    "site",
    "generator",
    "basic"
  ],
  "author": "Michael Erb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com"
  },
  "homepage": "https://github.com",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "autoprefixer": "^9.3.1",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.11",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "fs-extra": "^7.0.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "nunjucks": "^3.2.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "postcss-cli": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "sass-loader": "^7.0.3",
    "siema": "^1.5.1",
    "style-loader": "^0.21.0",
    "tailwindcss": "^0.7.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "public/*",
      "package.json",
      "commands/*",
      "views/*"
    ],
    "delay": "600"
  }
}
